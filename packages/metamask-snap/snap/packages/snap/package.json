{
  "name": "@cypher-laboratory/alicesring-snap",
  "version": "0.3.10",
  "description": "Use SAG and LSAG signatures to create privacy enhanced DApps",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cypher-Laboratory/Alice-s-Ring-snap"
  },
  "license": "MIT",
  "main": "./dist/bundle.js",
  "files": [
    "dist/",
    "images/",
    "snap.manifest.json"
  ],
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "build": "mm-snap build",
    "build:clean": "yarn clean && yarn build",
    "clean": "rimraf dist",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "prepublishOnly": "mm-snap manifest",
    "serve": "mm-snap serve",
    "start": "mm-snap watch",
    "test": "jest"
  },
  "dependencies": {
    "@metamask/snaps-sdk": "^6.5.1",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@metamask/auto-changelog": "^3.4.4",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-jest": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@metamask/snaps-cli": "^6.1.0",
    "@metamask/snaps-jest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsdoc": "^50.2.4",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=18.6.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
